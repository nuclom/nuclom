{
  "name": "nuclom",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "license": "Elastic-2.0",
  "packageManager": "pnpm@10.27.0",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "tsc": "turbo tsc",
    "lint": "biome check . --error-on-warnings",
    "lint:fix": "biome check . --write --error-on-warnings",
    "format": "biome format . --write",
    "test": "turbo test",
    "test:e2e": "turbo test:e2e",
    "db:generate": "pnpm --filter saas db:generate",
    "db:migrate": "pnpm --filter saas db:migrate",
    "db:push": "pnpm --filter saas db:push",
    "db:studio": "pnpm --filter saas db:studio",
    "prepare": "lefthook install"
  },
  "dependencies": {},
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@typescript/native-preview": "^7.0.0-dev",
    "@vercel/microfrontends": "^2.0.0",
    "lefthook": "^2.0.15",
    "turbo": "^2.7.4",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "overrides": {
      "undici@>=4.5.0 <5.28.5": ">=5.28.5",
      "esbuild@<=0.24.2": ">=0.25.0",
      "undici@<5.29.0": ">=5.29.0",
      "diff@<8.0.3": ">=8.0.3",
      "undici@<6.23.0": ">=6.23.0",
      "devalue@>=5.3.0 <=5.6.1": ">=5.6.2"
    },
    "onlyBuiltDependencies": [
      "lefthook"
    ]
  }
}
