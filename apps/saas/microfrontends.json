{
  "$schema": "https://openapi.vercel.sh/microfrontends.json",
  "applications": {
    "nuclom": {
      "packageName": "saas"
    },
    "saas": {
      "development": {
        "fallback": "https://app.nuclom.com"
      }
    },
    "marketing": {
      "routing": [
        {
          "paths": [
            "/",
            "/home",
            "/features",
            "/pricing",
            "/contact",
            "/support",
            "/brand",
            "/terms",
            "/privacy",
            "/cookies",
            "/content-policy",
            "/status"
          ]
        }
      ],
      "development": {
        "fallback": "https://nuclom.com"
      }
    }
  }
}
